import java.util.*;
class java{
    public static void main(String args[]){
        int[] arr= {1,0,2,3,0,0,4};
        int non_zero=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i]!=0){
                arr[non_zero]=arr[i];
                non_zero++;
            }
        }
        for(int i=non_zero;i<arr.length;i++){
            arr[i]=0;
        }
        System.out.print(Arrays.toString(arr));
    }
    
}
